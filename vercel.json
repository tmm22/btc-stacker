{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "devCommand": "bun run dev",
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 9 * * *"
    }
  ],
  "env": {
    "ENCRYPTION_KEY": "@encryption-key",
    "CRON_SECRET": "@cron-secret"
  }
}
