# Required: 64-character hex string for AES-256-GCM encryption
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=

# Required: Secret token for Vercel cron job authentication
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
CRON_SECRET=

# Required: Convex deployment URL (client-side)
NEXT_PUBLIC_CONVEX_URL=

# Required: Convex deployment URL (server-side)
CONVEX_URL=
